{
  "build": {
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Chatgpt Assistant",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "http": {
        "all": true,
        "request": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [],
    "security": {
      "csp": null
    },
    "bundle": {
      "resources": [
        "config"
      ],
      "active": true,
      "targets": "all",
      "identifier": "Chatgpt-Assistant",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}